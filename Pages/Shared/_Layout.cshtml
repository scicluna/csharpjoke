<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - JokeApp</title>
    <link rel="stylesheet" href="~/css/output.css" />
</head>

<body>
    <header>
        <nav class="">
            <div class="flex sm:text-5xl text-xl gap-5 justify-center">
                <a class="" asp-area="" asp-page="/Index">JokeApp</a>
                <a class="" asp-area="" asp-page="/Index">Home</a>
                <a class="" asp-area="" asp-page="/Privacy">Privacy</a>
            </div>
        </nav>
    </header>
    <main role="main" class="p-10">
        @RenderBody()
    </main>

    <footer class="p-10">
        &copy; 2023 - JokeApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </footer>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>